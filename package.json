{
    "name": "webapp-boilerplate",
    "version": "2.3.0",
    "description": "Boilerplate for Node.js Webapplications with Express.js, Marko.js & Webpack",
    "main": "dist/server.js",
    "scripts": {
        "start": "cross-env NODE_ENV=production node -r dotenv/config dist/server.js",
        "build": "cross-env NODE_ENV=production webpack --progress",
        "dev": "cross-env NODE_ENV=development webpack --progress && cross-env NODE_ENV=development nodemon -r dotenv/config dist/server.js",
        "debug": "cross-env NODE_ENV=development webpack --progress && cross-env NODE_ENV=development nodemon --inspect -r dotenv/config dist/server.js",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Teddy95/webapp-boilerplate.git"
    },
    "author": "Andre Sieverding",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Teddy95/webapp-boilerplate/issues"
    },
    "homepage": "https://github.com/Teddy95/webapp-boilerplate#readme",
    "devDependencies": {
        "@babel/core": "^7.10.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
        "@babel/preset-env": "^7.10.2",
        "@marko/webpack": "^1.2.5",
        "babel-loader": "^8.1.0",
        "browserslist-useragent-regexp": "^1.3.1-beta",
        "css-hot-loader": "^1.4.4",
        "css-loader": "^3.5.3",
        "eslint": "^6.8.0",
        "file-loader": "^4.3.0",
        "jest": "^25.5.4",
        "marko": "^4.22.0",
        "mini-css-extract-plugin": "^0.8.2",
        "node-sass": "^4.14.1",
        "nodemon": "^2.0.4",
        "pre-build-webpack": "^0.1.0",
        "sass-loader": "^8.0.2",
        "svg-url-loader": "^3.0.3",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-middleware": "^3.7.2",
        "webpack-dev-server": "^3.11.0",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-node-externals": "^1.7.2"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.13.0",
        "@primer/css": "^14.4.0",
        "body-parser": "^1.19.0",
        "cookie-parser": "^1.4.5",
        "cross-env": "^6.0.3",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "i18n-for-browser": "^1.2.2",
        "jquery": "^3.5.1",
        "octicons-marko": "^2.0.0",
        "passport": "^0.4.1",
        "passport-github": "^1.1.0"
    }
}
